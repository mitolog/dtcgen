{
  "name": "sketch-linter",
  "version": "0.0.0",
  "description": "sketch linter",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/jest": "~23.3.2",
    "@types/node": "~8.10.30",
    "jest": "~23.6.0",
    "prettier": "1.14.3",
    "rimraf": "~2.6.2",
    "ts-jest": "~23.10.2",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-microsoft-contrib": "~5.2.1",
    "tsutils": "~3.0.0",
    "typescript": "~3.1.0"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage --rootDir \"./core\"",
    "test:watch": "jest --watch --rootDir \"./core\""
  },
