{
  "sketch": {
    "extraction": {
      "keywords": [
        "Button",
        "TextView",
        "TextInput",
        "Image",
        "List",
        "Map",
        "NavigationBar"
      ],
      "dynamicClasses": [
        { "name": "Cell", "excludeOnPaste": false },
        { "name": "NavigationBar", "excludeOnPaste": true, "shift": "UP" }
      ],
      "exceptions": [
        "iPhone X Frame",
        "Color",
        "Overrides",
        "Tint",
        "Mask",
        "Pin",
        "Guide",
        "Tab Bar",
        "Keyboard"
      ],
      "followOverrides": true,
      "symbols": {
        "Button": {
          "Label": "text",
          "Background": "rectangle",
          "Icon": "symbolInstance"
        },
        "TextView": {
          "Description": "text",
          "Background": "rectangle"
        },
        "TextInput": {
          "Placeholder": "text",
          "Background": "rectangle"
        },
        "Image": {
          "Image": "rectangle",
          "Background": "rectangle"
        },
        "List": {
          "Cell": "symbolInstance",
          "Background": "rectangle"
        },
        "Map": {
          "Map": "symbolInstance"
        },
        "NavigationBar": {
          "Background": "rectangle",
          "Center": "symbolInstance",
          "Left": "symbolInstance",
          "Right": "symbolInstance"
        }
      }
    },
    "namingRule": {
      "page": {
        "pattern": "%s",
        "upperCamelCase": false,
        "englishOnly": true,
        "unique": true,
        "maxLength": 20
      },
      "artboard": {
        "pattern": "%s / %s",
        "lowerCamelCase": true,
        "englishOnly": true,
        "unique": true,
        "maxLength": 50
      }
    },
    "slice": {
      "caseSensitive": true,
      "keywords": ["Icons"],
      "extension": "PDF",
      "sliceAllImages": true,
      "scales": [1, 2, 3]
    }
  },
  "figma": {
    "slice": {
      "caseSensitive": true,
      "keywords": ["Icons"],
      "extension": "PNG",
      "sliceAllImages": false,
      "scales": [1, 2, 3]
    }
  }
}
